package Programs;

import java.util.Scanner;

public class Factorial_of_number {
	public static void main(String agrs[]) {
		Scanner sc=new Scanner(System.in);
		int factorial=1;
		int num=sc.nextInt();
		for(int i=num;i>0;i--) {
			 factorial=factorial*i;
		//	 System.out.print(factorial);
		}
		System.out.println(factorial);
		
	}
}
