package interview;

import java.util.Scanner;

public class Test {
	public static void main(String agrs[] ) {
		String input="Interview";
		String s=input.toLowerCase();
		char[] c=s.toCharArray();
		String reverse="";
		for(int i=c.length;i<0;i--) {
			reverse+=c;
		}
		System.out.println(reverse);
	}
}